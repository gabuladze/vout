extends ../layout.pug

block content
  div(class="row")
    div(class="col-lg-4")
    div(class="col-lg-4")
      form
        div(class="form-group")
          label(for="email") Email
          input(type="email", class="form-control", id="email", name="email", placeholder="example@examp.le")
        div(class="form-group")
          label(for="password") Password
          input(type="password", class="form-control", id="password", name="password", placeholder="passwrd")
        button(type="submit", class="btn btn-default") Log In
